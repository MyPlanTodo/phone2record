<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="sso-manager" extends="struts-default" namespace="/pri/admin">
		<action name="getSsoSystemList" class="com.manager.action.SsoSystemAction" method="getSsoSystemList"> 
			<result name="success">/pages/admin/pri/manager/sso_system_list.jsp</result>
		</action>
		
		<action name="toConfigSsoSystem" class="com.manager.action.SsoSystemAction" method="toConfigSsoSystem"> 
			<result name="success">/pages/admin/pri/manager/config_sso_system.jsp</result>
		</action>
		
		<action name="configSsoSystem" class="com.manager.action.SsoSystemAction" method="configSsoSystem"> 
			<result name="success">/pages/admin/pri/manager/sso_system_list.jsp</result>
		</action>
		
	</package>
