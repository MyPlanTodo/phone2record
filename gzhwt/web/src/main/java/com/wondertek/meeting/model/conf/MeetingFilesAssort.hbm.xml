<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wondertek.meeting.model.MeetingFilesAssort" table="meeting_files_assort">
	
		<id name="id" type="java.lang.Long">
			<column name="id" not-null="true" />
			<generator class="identity"></generator>
		</id>
		
		<property name="assortName" column="assort_Name" type="java.lang.String" />
		<property name="description" column="description" type="java.lang.String" />
		<property name="meetingId" column="meeting_Id" type="java.lang.Long" />
		<property name="creator" column="creator" type="java.lang.Long" />
		<property name="modifier" column="modifier" type="java.lang.Long" />
		<property name="modifyTime" column="modify_Time" type="java.util.Date" />
		<property name="applyDepartment" column="apply_Department" type="java.lang.String" />
		<property name="pageImgUrl" column="page_Img_Url" type="java.lang.String" />
		<property name="isImgFold" column="is_Img_Fold" type="java.lang.Boolean" />
	</class>
</hibernate-mapping>