<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.wondertek.meeting.model">
	<class name="MeetingGroup" table="meeting_group">
		<id name="id" type="java.lang.Long">
			<column name="id" not-null="true" />
			<generator class="identity"></generator>
		</id>
		<property name="planId" column="plan_id" type="java.lang.Long" />
		<property name="relationId" column="relation_id" type="java.lang.Long" />
		<property name="type" column="type" type="java.lang.Integer" />
	</class>
</hibernate-mapping>