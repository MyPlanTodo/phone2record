<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wondertek.meeting.model.DataDictConfig" table="data_dict_config"
		dynamic-update="true" dynamic-insert="true">

		<cache usage="read-write"/>
		<id name="id" type="java.lang.Long">
			<column name="id" not-null="true" />
			<generator class="identity"></generator>
		</id>
		
		<property name="actType" column="act_type" type="java.lang.String" />
		<property name="dataType" column="data_type" type="java.lang.String" />
		<property name="dataCode" column="data_code" type="java.lang.String" />
		<property name="dataName" column="data_name" type="java.lang.String" />
		<property name="dataDesc" column="data_desc" type="java.lang.String" />
		<property name="condition" column="cond" type="java.lang.String" />
		<property name="dataValue" column="data_value" type="java.lang.String" />
		<property name="orderBy" column="order_by" type="java.lang.String" />
		<property name="remark" column="remark" type="java.lang.String" />
	</class>
</hibernate-mapping>