<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wondertek.meeting.model.MeetingModuleTitle" table="meeting_module_title"
		dynamic-update="true" dynamic-insert="true">

		<id name="id" type="java.lang.Long">
			<column name="id" not-null="true" />
			<generator class="identity"></generator>
		</id>
		
		<property name="meetingId" column="meeting_id" type="java.lang.Long" />
		<property name="baseModuleTitleId" column="base_module_title_id" type="java.lang.Long" />
		<property name="baseMenuId" column="base_menu_id" type="java.lang.Long" />
		<property name="name" column="name" type="java.lang.String" />
		<property name="titleName" column="title_name" type="java.lang.String" />
		<property name="keyName" column="key_name" type="java.lang.String" />
		<property name="terminalType" column="terminal_type" type="java.lang.String" />
	</class>
</hibernate-mapping>