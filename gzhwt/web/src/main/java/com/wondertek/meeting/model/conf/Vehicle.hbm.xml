<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.wondertek.meeting.model">
	<class name="Vehicle" table="vehicle">
		<id name="id" type="java.lang.Long">
			<column name="id" not-null="true" />
			<generator class="identity"></generator>
		</id>
		
		<property name="name" column="name" type="java.lang.String" />
		<property name="number" column="number" type="java.lang.String" />
		<property name="capacity" column="capacity" type="java.lang.Integer" />
		<property name="comments" column="comments" type="java.lang.String" />
		<property name="meetingId" column="meeting_id" type="java.lang.Long" />
		<property name="type" column="type" type="java.lang.String" />
	</class>
</hibernate-mapping>