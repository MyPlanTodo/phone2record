<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wondertek.meeting.model.custom.PageCustom" table="page_custom">
		<id name="id" type="java.lang.Long">
			<generator class="identity"></generator>
		</id>
		<property name="propertyName" column="property_name" type="java.lang.String" length="20" unique="true" not-null="true"/>
		<property name="propertyValue" column="property_value" type="java.lang.String" length="200" />
		<property name="description" column="description" type="java.lang.String" length="200" />
		
		<many-to-one name="meeting" column="meeting_id" lazy="false"></many-to-one>
	</class>
</hibernate-mapping>