<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wondertek.meeting.model.MeetingSmsTemplate" table="Meeting_Sms_Template">
		<id name="id" type="java.lang.Long">
			<column name="id" not-null="true" />
			<generator class="identity"></generator>
		</id>
		<property name="type" column="type" type="java.lang.Integer" />
		<property name="name" column="name" type="java.lang.String" />
		<property name="content" column="content" type="java.lang.String" />
		<property name="sign" column="sign" type="java.lang.String" />
		<property name="meetingId" column="meeting_id" type="java.lang.Long" />
	</class>
</hibernate-mapping>