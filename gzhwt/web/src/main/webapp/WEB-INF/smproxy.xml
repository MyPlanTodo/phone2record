<?xml version="1.0" encoding="GB2312"?>
<!-- SM Proxy 配置文件-->
<config>
	<!--与ISMG(Internet Short Message Gateway)建立连接所需参数-->
	<CMPP3Connect>
		<!-- 业务数据库IP -->
		<biz_database_ip>10.196.136.96</biz_database_ip>
		<!-- 业务数据库名称 -->
		<biz_database_name>meeting_guizhou</biz_database_name>
		<!--业务数据库账号-->
		<biz_database_user>meeting</biz_database_user>
		<!--业务数据库密码-->
		<biz_database_passwd>meeting</biz_database_passwd>
	
		<!-- ISMG主机地址 -->
		<host>58.42.238.196</host>
		<!-- ISMG主机端口号 -->
		<port>7890</port>
		<!-- 心跳信息发送间隔时间(单位:秒) -->
		<heartbeat-interval>5</heartbeat-interval>
		<!-- 连接中断时重连间隔时间(单位:秒)-->
		<reconnect-interval>2</reconnect-interval>
		<!-- 需要重连时,连续发出心跳而没有接收到响应的个数(单位:个)-->
		<heartbeat-noresponseout>30</heartbeat-noresponseout>
		<!-- 操作超时时间(单位:秒) -->
		<transaction-timeout>60</transaction-timeout>
		<!--登陆网关的用户名-->
		<source-addr>EIE</source-addr>
		<!--双方协商的版本号(大于0,小于256)-->
		<version>0</version>
		<!--登陆网关的密码shared-secret由中国移动与ICP事先商定-->
		<shared-secret>123456</shared-secret>
		<!--账号下的接口个数-->
		<interface-number>1</interface-number>
		<!--企业代码,就是msg－src.SP…ID(最大为六位字符)-->
		<sp-id>424048</sp-id>
		<!-- 物理连接读操作超时时间(单位:秒) -->
		<read-timeout>8</read-timeout>
		<!--服务代码-->
		<sp-code>106575660101</sp-code>
		<!--业务代码-->
		<service-id>MGZ4019902</service-id>		
		<!--没有ack的情况下，最大提交数目，滑动窗口机制使用-->
		<max-mt-queue-count>600</max-mt-queue-count>
		<!--协议支持的手机号段130*131*132*133*134*135*136*137*138*139-->
		<numberSegment>130*131*132*133*134*135*136*137*138*139*150*151*152*153*154*155*156*157*158*159*106*180*181*182*183*184*185*186*187*188*189</numberSegment>
		<!--是否是异步模式-->
		<asyncMode>true</asyncMode>
		<!--是否属于调试状态,true表示属于调试状态,所有的消息被打印输出到屏幕,
		        码流也会记录到日志中，false表示不属于调试状态,所有的消息不被输出-->
		<debug>true</debug>
		<!--协议开关-->
		<enable>true</enable>
		
	</CMPP3Connect>
</config>