	<!--add by guo -->
	<function>
    <description>
      change file postfix.
    </description>
    <name>getFileCssByFilePostfix</name>
    <function-class>com.wondertek.meeting.util.FileOperatorUtil</function-class>
    <function-signature>java.lang.String getFileCssByFilePostfix(java.lang.String)</function-signature>
    <example>
      ${wd:getFileCssByFilePostfix(postFix)}
    </example>
  </function>
  
	
	<!-- added by zhangguojing -->
	<function>
    <description>
      limit string value.
    </description>
    <name>limit</name>
    <function-class>com.wondertek.meeting.util.StringUtil</function-class>
    <function-signature>java.lang.String limit(java.lang.String, int)</function-signature>
    <example>
      ${wd:limit(title, 10)}
    </example>
  </function>
  
  <function>
    <description>
      Base64 decode.
    </description>
    <name>base64Decode</name>
    <function-class>com.wondertek.meeting.util.StringUtil</function-class>
    <function-signature>java.lang.String base64Decode(java.lang.String)</function-signature>
    <example>
      ${wd:base64Decode(content)}
    </example>
  </function>
  
  <function>
    <description>
      convert \n to br.
    </description>
    <name>convertToHtmlLine</name>
