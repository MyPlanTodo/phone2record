<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"    "http://struts.apache.org/dtds/struts-2.0.dtd"><struts>	<package name="public-base" extends="struts-base" namespace="/portal/base">		<action name="login" class="com.wondertek.meeting.action.login.LoginAction"			method="login">			<result name="input">/pages/portal/login.jsp</result>			<result name="admin" type="redirectAction">				<param name="namespace">/admin/pri/meeting</param>				<param name="actionName">listMeeting.action</param>			</result>			<!-- <result name="admin" type="redirect">/pages/admin/pri/frames/indexFrame.jsp</result> -->			<result name="portal" type="redirectAction">				<param name="namespace">/portal/pri/meeting</param>				<param name="actionName">getAttendMeetingList.action</param>			</result>			<result name="meeting" type="redirectAction">				<param name="namespace">/portal/pri/meeting</param>				<param name="actionName">getMeetingById.action</param>				<param name="returnType">portal</param>				<param name="from">portal</param>				<param name="meeting.id">${meeting.id}</param>			</result>		</action>		<action name="getPwd" class="com.wondertek.meeting.action.login.LoginAction"			method="getPwd">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>		<action name="goPortalLogin" class="com.wondertek.meeting.action.login.LoginAction"			method="goPortalLogin">			<result name="portal_login">/pages/portal/login.jsp</result>		</action>		<!--会场座位安排 add by zhoujian --><!-- 		<action name="managerseat_*" --><!-- 			class="com.wondertek.meeting.action.meeting.MeetingSeatAction" --><!-- 			method="{1}"> --><!-- 		</action> -->	</package>	<package name="admin-base" extends="struts-base" namespace="/admin/base">		<action name="login"			class="com.wondertek.meeting.action.login.AdminLoginAction" method="login">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>			<action name="ssologin"			class="com.wondertek.meeting.action.login.AdminLoginAction" method="ssologin">			<result name="success">/pages/admin/pri/index.jsp</result>			<result name="input">/pages/admin/SSOLogin.jsp</result>	</action>		<action name="checkCode"			class="com.wondertek.meeting.action.login.CheckCodeImageAction"			method="checkCode">		</action>		<!-- rfid scan tag trigger -->		<action name="rfidTagTrigger" class="com.wondertek.meeting.action.meeting.CardAction"			method="rfidTagTrigger">		</action>		<action name="checkMeetingId" class="com.wondertek.meeting.action.meeting.CardAction"			method="checkMeetingId">		</action>				<action name="querySignEvents" class="com.wondertek.meeting.action.meeting.CardAction"			method="querySignEvents">		</action>	</package>	<package name="client-base" extends="struts-base" namespace="/client/base">		<action name="login" class="com.wondertek.meeting.action.login.LoginAction"			method="clientLogin">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>		<action name="getPwd" class="com.wondertek.meeting.action.login.LoginAction"			method="getPwd">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>		<action name="checkUpdate" class="clientVersionAction" method="checkUpdate">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>		<action name="getBackgroundImage"			class="com.wondertek.meeting.action.config.SystemConfigAction"			method="getBackgroundImage">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>	</package>	<package name="meeting" extends="struts-base" namespace="/base">		<global-results>			<result name="message">/pages/portal/common/message.jsp</result>		</global-results>		<action name="viewMeeting"			class="com.wondertek.meeting.action.meeting.MeetingAction" method="viewMeeting">			<result>/pages/portal/viewMeeting.jsp</result>		</action>		<action name="applyMeetingReq"			class="com.wondertek.meeting.action.meeting.MeetingAction" method="applyMeetingReq">			<result>/pages/portal/applyMeeting.jsp</result>		</action>		<action name="applyMeeting"			class="com.wondertek.meeting.action.meeting.MeetingAction" method="applyMeeting">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>		<action name="quickEnterMeeting"			class="com.wondertek.meeting.action.meeting.MeetingAction" method="quickEnterMeeting">			<result name="input">/pages/portal/viewMeeting.jsp</result>			<result name="enterMeeting" type="redirectAction">				<param name="namespace">/portal/pri/meeting</param>				<param name="actionName">getMeetingById.action</param>				<param name="returnType">portal</param>				<param name="from">portal</param>				<param name="meeting.id">${meeting.id}</param>			</result>		</action>	</package></struts>