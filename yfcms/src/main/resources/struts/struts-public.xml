<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"    "http://struts.apache.org/dtds/struts-2.0.dtd"><struts>	<package name="admin-base" extends="struts-base" namespace="/admin/base">		<action name="login" class="com.wondertek.meeting.action.login.AdminLoginAction"			method="login">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>				<action name="ssologin" class="com.wondertek.meeting.action.login.AdminLoginAction"			method="ssologin">			<result name="success">/pages/admin/pri/index.jsp</result>			<result name="input">/pages/admin/login.jsp</result>		</action>				<action name="checkCode"			class="com.wondertek.meeting.action.login.CheckCodeImageAction"			method="checkCode">		</action>			 </package>		<package name="client-base" extends="struts-base" namespace="/client/base">		<action name="login" class="com.wondertek.meeting.action.login.LoginAction"			method="clientLogin">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>				<action name="getPwd" class="com.wondertek.meeting.action.login.LoginAction"			method="getPwd">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>				<action name="checkUpdate" class="clientVersionAction"			method="checkUpdate">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>				<action name="getBackgroundImage" class="com.wondertek.meeting.action.config.SystemConfigAction"			method="getBackgroundImage">			<result type="json"><param name="root">resultMap</param></result>		</action>	</package>		<package name="meeting" extends="struts-base" namespace="/meeting/base">		<global-results>			<result name="message">/pages/portal/common/message.jsp</result>		</global-results>				<action name="viewMeeting" class="com.wondertek.meeting.action.meeting.MeetingAction" method="viewMeeting">			<result>/pages/portal/viewMeeting.jsp</result>		</action>				<action name="applyMeetingReq" class="com.wondertek.meeting.action.meeting.MeetingAction" method="applyMeetingReq">			<result>/pages/portal/applyMeeting.jsp</result>		</action>				<action name="applyMeeting" class="com.wondertek.meeting.action.meeting.MeetingAction" method="applyMeeting">			<result type="json">				<param name="root">resultMap</param>			</result>		</action>		<action name="quickEnterMeeting" class="com.wondertek.meeting.action.meeting.MeetingAction" method="quickEnterMeeting">			<result name="input">/pages/portal/viewMeeting.jsp</result>			<result name="enterMeeting" type="redirectAction">				<param name="namespace">/portal/pri/meeting</param>				<param name="actionName">getMeetingById.action</param>				<param name="returnType">portal</param>				<param name="from">portal</param>				<param name="meeting.id">${meeting.id}</param>			</result>		</action>			</package></struts>