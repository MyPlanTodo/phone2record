<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wondertek.meeting.model.MeetingClientMenu" table="meeting_client_menu">

		<composite-id>
			<key-property name="meetingId" column="meeting_id" type="java.lang.Long" />
			<key-property name="menuId" column="menu_Id" type="java.lang.Long" />
			<key-property name="memberLevel" column="member_Level" type="java.lang.Integer" />
		</composite-id>
		
		<property name="sort" column="sort" type="java.lang.Integer" />
		<property name="terminalType" column="terminal_Type" type="java.lang.String" />
		
	</class>
</hibernate-mapping>