<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wondertek.meeting.model.SmsSendTemplate" table="Sms_Send_Template">

		<id name="id" type="java.lang.Long">
			<column name="id" not-null="true" />
			<generator class="identity"></generator>
		</id>
		
		<property name="type" column="type" type="java.lang.String" />
		<property name="title" column="title" type="java.lang.String" />
		<property name="content" column="content" type="java.lang.String" />
		<property name="state" column="state" type="java.lang.Integer" />
		<property name="optuser" column="optuser" type="java.lang.Long" />
		<property name="optdate" column="optdate" type="java.util.Date" />
		
	</class>
</hibernate-mapping>