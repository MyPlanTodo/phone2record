<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.guo.yf.model.security.SecurityRole" table="Security_Role"
		dynamic-update="true" dynamic-insert="true">
		<id name="id" type="java.lang.Long">
			<column name="id" not-null="true" />
			<generator class="identity"></generator>
		</id>
		
		<property name="roleName" column="role_Name" type="java.lang.String" />
		<property name="roleDescription" column="role_Description" type="java.lang.String" />
		
		<property name="createTime" column="create_time" type="java.util.Date" />
		<property name="creator" column="creator" type="java.lang.Long" />
		<property name="modifyTime" column="modify_time" type="java.util.Date" />
		<property name="modifier" column="modifier" type="java.lang.Long" />
		
	</class>
</hibernate-mapping>