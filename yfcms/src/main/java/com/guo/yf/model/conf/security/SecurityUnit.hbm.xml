<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.guo.yf.model.security.SecurityUnit" table="Security_Unit"
		dynamic-update="true" dynamic-insert="true">
		<id name="id" type="java.lang.Long">
			<column name="id" not-null="true" />
			<generator class="identity"></generator>
		</id>
		
		<property name="unitName" column="unit_Name" type="java.lang.String" />
		<property name="unitDescription" column="unit_Description" type="java.lang.String" />
		<property name="unitType" column="unit_Type" type="java.lang.String" />
		<property name="unitUrl" column="unit_Url" type="java.lang.String" />
		<property name="unitParentId" column="unit_Parent_Id" type="java.lang.String" />
		<property name="modifier" column="modifier" type="java.lang.Long" />
		<property name="createTime" column="create_time" type="java.util.Date" />
		<property name="creator" column="creator" type="java.lang.Long" />
		<property name="modifyTime" column="modify_time" type="java.util.Date" />
		<property name="orderCode" column="order_Code" type="java.lang.Integer" />
		<property name="returnDefaultMenu" column="return_default_menu" type="java.lang.Boolean" />
		
	</class>
</hibernate-mapping>